            <xml-property name="queryText"><![CDATA[select cn.TRADEDATE_, cn.ACCOUNTNAME_, ter.DESCRIPTION_ as 'Root Cause'
	, ( CASE cn.TYPE_ WHEN 'Regular' THEN 'Chapel Hill Denham Securities' ELSE cn.CUSTODIANNAME_ END ) CUSTODIANNAME_
	, cn.ERRORNARRATIVE_ as 'Reason', cn.ERRORCORRECTIVENARRATIVE_ as 'Corr Narratn'
	, cn.FINANCIALIMPLICATIONCREDIT_ as 'Credit', cn.FINANCIALIMPLICATIONDEBIT_ as 'Debit'
	, cn.PAYMENTINSTRUCTIONNARRATIVE_ as 'Payment', cn.RESPONSIBLEPARTY_ as 'Party Resp'
from CONTRACTNOTE_ cn
	left join TRADEERRORREASON_ ter
		on cn.ERRORROOTCAUSE_ID=ter.TRADEERRORREASON_ID
where cn.INCLUDEINERRORREPORT_=1
	AND cn.TRADEDATE_ BETWEEN ? AND ?
	AND cn.LOCALE_ = ? ]]></xml-property>
