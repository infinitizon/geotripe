            <xml-property name="queryText"><![CDATA[SELECT cp.CUSTOMERPROFILE_ID, cp.CUSTOMERNAME_, cp.CUSTOMERID_
	, (CASE WHEN cp.ADDRESSLINE1_ IS NULL THEN '' ELSE cp.ADDRESSLINE1_ END) ADDRESSLINE1_
	, (CASE WHEN cp.ADDRESSLINE2_ IS NULL THEN '' ELSE cp.ADDRESSLINE2_ END) ADDRESSLINE2_
	, (CASE WHEN cp.CITY_ IS NULL THEN '' ELSE cp.CITY_ END) CITY_
	, (CASE WHEN cp.STATE_ IS NULL THEN '' ELSE cp.STATE_ END) STATE_
	, (CASE WHEN cp.COUNTRY_ IS NULL THEN '' ELSE cp.COUNTRY_ END) COUNTRY_
	, (CASE WHEN cp.EMAILADDRESS_ IS NULL THEN '' ELSE cp.EMAILADDRESS_ END) EMAILADDRESS_
	, (CASE WHEN cp.MOBILENUMBER_ IS NULL THEN '' ELSE cp.MOBILENUMBER_ END) MOBILENUMBER_
	, cp.ENTRYDATE_
FROM CUSTOMERPROFILE_ cp
WHERE cp.CUSTOMERID_ = ?]]></xml-property>
