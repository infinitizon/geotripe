            <xml-property name="queryText"><![CDATA[SELECT a.CUSTOMER_PORTFOLIO_ID, pd.EFFECTIVEDATE_, pd.DATE_, pd.DESCRIPTION_
	, pd.AMOUNT_
	, pd.TRANSACTIONTYPE_ID
	, (SELECT NAME_ FROM TRANSACTIONTYPE_ WHERE TRANSACTIONTYPE_ID = pd.TRANSACTIONTYPE_ID) ActivityType
FROM POSITIONDETAIL_ pd
	JOIN FINANCIALPOSITION_ fp
		ON pd.FINANCIALPOSITION_DETAILS_ID=fp.FINANCIALPOSITION_ID
	JOIN ACCOUNT_ a
		ON a.ACCOUNT_ID=fp.PORTFOLIO_POSITIONS_ID
WHERE pd.CLASSTYPE_ID=2
AND  pd.SHOWONSTATEMENT_ = 1
AND a.CUSTOMER_PORTFOLIO_ID = ? 
AND pd.EFFECTIVEDATE_ BETWEEN ? AND ?
AND a.PRODUCT_ID IN (655949333,866222080,911179776,655949824,655949827,655949831)
		]]></xml-property>
