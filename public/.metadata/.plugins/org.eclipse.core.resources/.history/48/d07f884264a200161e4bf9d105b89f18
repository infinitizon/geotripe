            <xml-property name="queryText"><![CDATA[SELECT a.CUSTOMER_PORTFOLIO_ID, ah.AVAILABLEQUANTITY_, ah.TOTALQUANTITY_
	, ah.TOTALCOST_, ah.GAINLOSSMONETARY_, ah.GAINLOSSPERCENTAGE_, ah.PRICE_
	, ah.SECURITYTYPE_, ah.TICKER_, ah.AVERAGEPRICE_, ah.ENTRYDATE_
	, ah.MARKETPRICE_, MARKETVALUE_
FROM ACCOUNTHOLDINGS_ ah
	JOIN ACCOUNT_ a
		ON ah.ACCOUNTID_=a.ACCOUNT_ID
WHERE a.CUSTOMER_PORTFOLIO_ID = ?
	AND ah.TOTALQUANTITY_<>0
	AND ah.ENTRYDATE_= 
	(SELECT MAX(ah_.ENTRYDATE_)
	FROM ACCOUNTHOLDINGS_ ah_
		JOIN ACCOUNT_ a_
			ON ah_.ACCOUNTID_=a_.ACCOUNT_ID
	WHERE a_.CUSTOMER_PORTFOLIO_ID = ?
	)]]></xml-property>
