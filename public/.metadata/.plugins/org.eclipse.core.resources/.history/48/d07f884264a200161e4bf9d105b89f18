            <xml-property name="queryText"><![CDATA[SELECT a.CUSTOMER_PORTFOLIO_ID, pd.EFFECTIVEDATE_, pd.DATE_
	, (SELECT INTERNALIDENTIFIER_ FROM FINANCIALEXPOSURE_ WHERE FINANCIALEXPOSURE_ID=fp.EXPOSURE_ID) SECURITY_
	, (CASE WHEN pd.PURCHASEPRICE_ IS NULL THEN pd.SALEPRICE_ ELSE pd.PURCHASEPRICE_ END)PURCHASEPRICE_
	, pd.QUANTITY_, pd.CURRENTQUANTITY_, ((CASE WHEN pd.PURCHASEPRICE_ IS NULL THEN pd.SALEPRICE_ ELSE pd.PURCHASEPRICE_ END) * pd.CURRENTQUANTITY_)PURCHASEVALUE_
	, pd.SALEPRICE_, pd.SALEVALUE_, pd.ORIGINALQUANTITY_, pd.CHARGES_, pd.TYPE_ID, pd.TYPE_ID, pd.TOTAL_
	, (SELECT NAME_ FROM SECURITYPOSITIONDETAILTYPE_ WHERE SECURITYPOSITIONDETAILTYPE_ID = pd.TYPE_ID) type_
FROM POSITIONDETAIL_ pd
	JOIN FINANCIALPOSITION_ fp
		ON pd.FINANCIALPOSITION_DETAILS_ID=fp.FINANCIALPOSITION_ID
	JOIN ACCOUNT_ a
		ON a.ACCOUNT_ID=fp.PORTFOLIO_POSITIONS_ID
WHERE pd.CLASSTYPE_ID=4
AND a.CUSTOMER_PORTFOLIO_ID = ? 
AND pd.TYPE_ID IN (1,2) AND pd.STATUS_ID IN (3001, 3005, 3006)
AND pd.EFFECTIVEDATE_ BETWEEN ? AND ?]]></xml-property>
