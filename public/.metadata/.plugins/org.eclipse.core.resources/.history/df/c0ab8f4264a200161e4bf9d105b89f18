            <xml-property name="queryText"><![CDATA[SELECT pd.POSITIONDETAIL_ID, pd.`ACCOUNTNAME_`, pd.`ISSUEDATE_`, pd.`INSTRUMENT_ID`, pd.`INSTRUMENTTYPE_`, pd.`DEALNUMBER_`,  pd.`TENOR_`, p2.`NAME_` as issuer
	, pd.`PRINCIPALAMOUNT_`, pd.`COMMISSIONRATE_`, pd.`COMMISSION_`, pd.`INTERESTRATE_`, pd.`INTEREST_`, pd.`NETINTEREST_`
	, p.`ADDRESSLINE1_`, p.`ADDRESSLINE2_`, p.`ADDRESSCITY_`, pd.`UPFRONTINTERESTPAID_`, pd.`MATURITYDATE_`, pd.`LOCALE_`
	, (select `NAME_` from GEOGRAPHICBOUNDARY_ where `GEOGRAPHICBOUNDARY_ID`=p.`ADDRESSSTATE_ID`) state
	, (select `NAME_` from GEOGRAPHICBOUNDARY_ where `GEOGRAPHICBOUNDARY_ID`=p.`ADDRESSCOUNTRY_ID`) country
FROM POSITIONDETAIL_ pd
	JOIN FINANCIALPOSITION_ fp
		ON pd.FINANCIALPOSITION_DETAILS_ID=fp.FINANCIALPOSITION_ID
	JOIN ACCOUNT_ a
		ON a.ACCOUNT_ID=fp.PORTFOLIO_POSITIONS_ID
	LEFT JOIN PARTYROLE_ pr
		ON a.`CUSTOMER_PORTFOLIO_ID`=pr.`PARTYROLE_ID`
	LEFT JOIN PARTY_ p
		ON pr.`PARTY_ROLES_ID`=p.`PARTY_ID`
	LEFT JOIN `FINANCIALEXPOSURE_` fe
		ON fp.`EXPOSURE_ID` = fe.`FINANCIALEXPOSURE_ID`
	LEFT JOIN `PARTY_` p2
		ON fe.`ISSUER_ID` = p2.`PARTY_ID`
WHERE pd.POSITIONDETAIL_ID='2506883075']]></xml-property>
