            <xml-property name="queryText"><![CDATA[SELECT q.quote_Id, q.rfq_no, q.po_no, qd.split_po_no, qd.partno_modelno, qd.description, qd.oem_description
	, ifnull(p.name,'') clientname, p.contactphonenumber, ifnull(p.addressline1,'')addressline1, ifnull(p.addressline2,'')addressline2
	, ifnull(p.addresscity,'')addresscity, ifnull(s.name,'') state, ifnull(c.name,'') country, p.emailaddress
	, (CASE WHEN qd.oem_description IS NULL OR qd.oem_description = '' THEN qd.description ELSE qd.oem_description END) 'final_desc' 
	, qd.quantity, ifnull(uom.name,'') unitofmeasure, qd.unitprice, (qd.quantity*qd.unitprice) extAmt
    , ifnull(cur.code,'')cur_code, ifnull(cur.description,'')cur_desc, cur.decimalHTML, cur.hexHTML, cur.unicode
FROM Quote q
	JOIN QuoteDetail qd ON q.quote_id=qd.Quote_quote_Id
	LEFT JOIN QuoteDetail_Manufacturer qdm ON qd.Quote_quote_Id=qdm.QuoteDetail_QuoteDetail_Id
	LEFT JOIN Party p ON qdm.Party_Party_Id=p.Party_Id 
    LEFT JOIN State s ON p.party_state_id=s.state_id
    LEFT JOIN Country c ON p.party_country_id=c.country_id
	LEFT JOIN unitofmeasure uom ON qd.unitofmeasure = uom.unitofmeasure_id
    LEFT JOIN Currency cur ON q.quote_currency_id=cur.currency_id
WHERE qd.quote_is_po=1
	AND IFNULL(q.po_no,1) LIKE ?
	AND IFNULL(qd.split_po_no,1) LIKE ?;
]]></xml-property>
