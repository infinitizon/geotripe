            <method name="onFetch"><![CDATA[var monthNames = [
  "January", "February", "March",
  "April", "May", "June", "July",
  "August", "September", "October",
  "November", "December"
];

var date = new Date();
var day = date.getDate();
var monthIndex = date.getMonth();
var year = date.getFullYear();
reportContext.setPersistentGlobalVariable("rptTitle", "Purchase Order");
reportContext.setPersistentGlobalVariable("poNo", "PO NO. "+row["po_no"] +" | " + day +" "+ monthNames[monthIndex]+" "+year);
reportContext.setPersistentGlobalVariable("currency", "CURRENCY "+row["cur_code"]+" ("+row["decimalHTML"]+")");]]></method>
